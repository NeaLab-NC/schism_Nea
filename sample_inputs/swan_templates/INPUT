PROJ 'my_fake_project' '01'
$
$------- SET  setting physical parameters and error counters
$
$SET LEVEL = 0.0
$SET NOR = 90.0
$SET DEPMIN = 0.05
$SET MAXMES = 0
SET MAXERR = 3
$SET GRAV = 9.81
$SET RHO = 1025.00
$SET INRHOG = 0
$SET HSRERR = 0.1
$SET PWTAIL = 3.0
$SET FROUDMAX = 0.8
$SET SORT = -999.0
SET PRINTF = 4
SET PRTEST = 5
SET NAUT
MODE NONSTATIONARY

COORDINATES CARTESIAN
$
$
$ ********************MODEL INPUT************************
$
$ ****************  Computation Grid. *******************
$
CGRID UNSTRUCTURED CIRCLE MDC=36 FLOW=0.031 FHIGH=1.42 MSC=29

$
&& KEYWORD TO CREATE CURRENT GRID &&
$INPGRID CURRENT CURVILINEAR 0 0 500 135 EXC 9.999000e+003              &
$        NONSTAT  20100801.000000 1 DAY 20100901.000000

&& KEYWORD TO CREATE WATER LEVEL GRID &&
$INPGRID WLEV CURVILINEAR 0 0 500 135 EXC 9.999000e+003                 &
$        NONSTAT  20100801.000000 1 DAY 20100901.000000

&& KEYWORD TO CREATE BOTTOM FRICTION GRID &&
$INPGRID FRIC CURVILINEAR 0 0 500 135 EXC 9.999000e+003                 &
$        NONSTAT  20100801.000000 1 DAY 20100901.000000

&& Wind forcing &&
$INPGRID WIND REGULAR 155000.0 3200000.0 0 92 32 6350.0 7400.0 EXC 9.999000e+003
$READINP WIND  1 'C:\Users\rmickey\Documents\SWAN_ngomex\breton\forcings\swan_wind\H1_D1_wind.dat' 4 0 FREE

&&INPGRID WIND 276.459991 24.100000 0. 209 161 0.024663 0.022500 NONSTAT 20100301.1800 #TSTEP#.0 HR 20100310.1200
&&READINP WIND 1.0 '2010030118.wnd' 3 0 0 0 FREE
$

$ ======== Section 3. Input ===============================

SCGRID INP_CUR = F
!$$if INP_CUR == T
SCGRID INP_CUR_STAT = F
SCGRID INP_CUR_NAME =  curr.xy
!$$if INP_CUR_STAT == F
