&& BOUNDARY FORCING &&
& --------------------------------
$
$ PREFER BOUN SEG NPTS IJ K" rather than "BOUN WWIII" since "BOUN WWIII" requires multiple locations in one file.
$
SCBOU BOUNDARY = defined     !default     !defined
!$$if BOUNDARY == defined
SCBOU BOUND_CHOICE = NESTING  !UNIFORM (or CONSTANT), NESTING
SCBOU NESTING_TYPE_WAVE = WAMPARAM
!SCBOU NESTING_FILE = WAVEFILE.dat
SCBOU NESTING_FILE = WAVEFILE.nc
SCBOU SHAPESPEC = JON       !JON, PM, GAUS, BIN
!$$if SHAPESPEC == JON
SCBOU GAMMA = 3.3
!$$else if SHAPESPEC = GAUS
!SCBOU SIGFR = 0.01
!$$end if SHAPESPEC
SCBOU CHAR_WAVE_PERIOD = PEAK      !PEAK, MEAN
SCBOU DSPR = POW                   !POW, DEGREES
SCBOU HSIG = 0.05
SCBOU PER = 10.0
SCBOU DIR = 185.0
SCBOU DD = 20.0
!$$endif BOUNDARY

$ ***************  PHYSICS  ***************************
$
GEN3 KOMEN AGROW
WCAP KOMEN 2.36E-5 3.02E-3 2.0 1.0 1.0
BREAKING CON 1.0 0.73
FRICTION MADSEN KN=0.05
PROP BSBT
TRIAD
$OFF QUAD
$OFF WINDG
$OFF REF
$
NUM STOPC DABS=0.005 DREL=0.01 CURVAT=0.005 NPNTS=95 NONSTAT MXITNS=25
$
$INIT ZERO
$
$************ Output REQUESTS *************************
$
$ POINTS 'BUOYS' FILE   'f31hari.loc'
$ TABLE  'BUOYS' HEAD   'f32har01.tab' DIST DEP HS RTP TM01 TM02 DSPR DIR FORCE
$ TABLE  'BUOYS' NOHEAD 'f32har01.tbl' HS RTP TPS TM01 TM02
$ SPEC   'BUOYS' SPEC1D 'f32har01.spc'
$
&&BLOCK 'COMPGRID' NOHEAD 'ouvea_test.nc' LAY 3 TIME XP YP HSIGN DIR TM01 DEPTH OUTPUT 20130101.000000 60 min
$
$SPECTRA OUT-LOCATIONS
$
$SPECTRA COMMANDS
$
$PARTITION OUTCOMMANDS
$

!======== Section 7. Command COMPUTE ==============================================
$ set itest=1 in order to get detailed information in PRINT file
$TEST 1,0 POI IJ 50 52 54 PAR 'TEST.PAR'
$TEST ITEST=1 ITRACE=1 POINT IJ 48260 47913 PAR 'TEST.PAR'
TEST ITEST=0 ITRACE=10 POINT IJ 500
COMPUTE 800 SEC

